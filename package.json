{
  "name": "gatsby-starter-strict-next",
  "description": "A Gatsby starter with a set of strict linting, auto-formatting rules and react 16+",
  "version": "0.0.1",
  "private": true,
  "author": "Christian Rebelsky <christian.rebelsky@gmail.com",
  "repository": "https://github.com/crebelsky/gatsby-starter-strict-next",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5.2.0"
  },
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "build:serve": "gatsby serve",
    "lint": "eslint --ignore-path .gitignore \"**/*.{js,jsx}\"",
    "format": "prettier-eslint  --config \"./.prettierrc\" --write \"src/**/*.{js,jsx,json,css,scss,md}\"",
    "test": "jest --config ./jest.config.json --no-cache",
    "test:cover": "npm run test -- --coverage",
    "storybook": "start-storybook -p 9001 -c storybook"
  },
  "dependencies": {
    "@storybook/addon-options": "3.3.15",
    "@storybook/react": "3.3.15",
    "gatsby": "1.9.231",
    "gatsby-link": "1.6.39",
    "gatsby-plugin-react-helmet": "2.0.7",
    "gatsby-plugin-react-next": "1.0.11",
    "gatsby-plugin-styled-components": "2.0.9",
    "grid-styled": "3.2.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-router-dom": "4.2.2",
    "styled-components": "3.2.1",
    "styled-normalize": "4.0.0"
  },
  "devDependencies": {
    "babel-jest": "22.4.1",
    "babel-loader": "7.1.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "jest": "22.4.2",
    "prettier-eslint-cli": "4.7.1",
    "react-test-renderer": "16.2.0"
  }
}
